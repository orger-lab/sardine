<local:SardineWindow x:Class="Sardine.Core.Views.WPF.SardineMainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sardine.Core.Views.WPF"
        mc:Ignorable="d"
        Title="SardineMainWindow" Height="400" MaxWidth="400" MinWidth="400"
                     Loaded="SardineMainWindow_Loaded">
    <Grid VerticalAlignment="Stretch">
        <Grid.RowDefinitions>
            <RowDefinition Height="20"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <local:HelpersMenu x:Name="HelperMenuBar" Grid.Column="0" Grid.Row="0"/>
        <local:FleetManagerUI x:Name="ManagerUI" Grid.Column="0" Grid.Row="1"/>
    </Grid>
</local:SardineWindow>
