<Window x:Class="Sardine.Core.Views.Logs.WPF.LoggerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sardine.Core.Views.Logs.WPF"
        mc:Ignorable="d"
        x:Name="logger"
        Title="Logger"
        ResizeMode="CanResize"
        MaxWidth="800"
        MaxHeight="1000"
        MinWidth="400"
        MinHeight="500"
        >
    <Grid>
        <local:LoggerUI x:Name="loggerUI" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>
    </Grid>
</Window>
